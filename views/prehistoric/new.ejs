<form action="/prehistoric" method="post">
    <div>
        <label for="dinosaurType">Image Url:</label>
        <input type="text" name="url" id="creatureImage">
    </div>

    <div>
        <label for="dinosaurName">Name:</label>
        <input type="text" name="name" id="creatureName">
    </div>

